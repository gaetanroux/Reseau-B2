$TTL 86400

@   IN  SOA     dns1.server1.tp3. root.server1.tp3. (
         2021080804  ;Serial
         3600        ;Refresh
         1800        ;Retry
         604800      ;Expire
         86400       ;Minimum TTL
)
        ; Set your Name Servers here
@         IN  NS      dns1.server1.tp3.
         ; define Name Server's IP address
@         IN  A       10.3.0.67

; Set each IP address of a hostname. Sample A records.
dhcp           IN A        10.3.0.3
router         IN A        10.3.0.2
